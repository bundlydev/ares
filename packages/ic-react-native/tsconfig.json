{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./build",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "declaration": true,
    "module": "ESNext",
  },
  "include": ["./src"],
  "exclude": ["node_modules", "build"],
}
